<div class="container">
  <h3>Manhole Covers in the Region of <%= @region.titleize %></h3>
  <% @certain_region_array.each_with_index do |manhole_entry| %>
    <a href="/users/<%= manhole_entry.user_id %>/manholecovers/<%= manhole_entry.id %>">
      <ul class="list-of-a-feature">
        <li><img src="<%= manhole_entry.manhole_img.url(:small) %>"></li>
      </ul>
    </a>
  <% end %>
  <div class="clear"></div>
</div>
