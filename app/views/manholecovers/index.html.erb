<section>
  <h3>Manhole Covers Collected by <%= @collector.name %></h3>
  <% @manholecovers.each do |manhole_entry| %>
    <a href="/collectors/<%= manhole_entry.collector_id%>/manholecovers/<%= manhole_entry["id"] %>">
      <ul class="list-of-a-feature">
        <li><img src="<%= manhole_entry.manhole_url %>"></li>
      </ul>
    </a>
  <% end %>
  <div class="clear"></div>
</section>
