<section>
  <h3>Manhole Covers with the keyword of <%= @keyword %></h3>
  <% @with_keyword_array.each_with_index do |manhole_entry| %>
    <a href="/manholecovers/<%= manhole_entry["id"] %>">
      <ul class="list-of-a-feature">
        <li><img src="<%= manhole_entry.manhole_url %>"></li>
      </ul>
    </a>
  <% end %>
  <div class="clear"></div>
</section>
