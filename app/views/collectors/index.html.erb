<section>
  <%= link_to image_tag("nonmanholes/close.png", :class => "close"), root_path%>
  <h3>Who Are The Collectors?</h3>
  <div id="collector-wall">
    <% @collectors.each do |collector| %>
      <a href="/collectors/<%= collector.id %>">
        <div id="collector-box">
          <%= image_tag(collector.avatar.url(:medium), :class => "collector-list-img") %>
        </div>
      </a>
    <% end %>
    <div class="clear"></div>
  </div>
</section>
